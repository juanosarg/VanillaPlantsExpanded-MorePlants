<?xml version="1.0" encoding="utf-8" ?>
<Defs>  


	<ThingDef ParentName="PlantFoodRawBase">
		<defName>VCE_RawCelery</defName>
		<label>celery</label>
		<description>A vegetable consisting of a long fibrous stalk tapering into leaves. The leaves are strongly flavored and are used less often, either as a flavoring in soups and stews or as a dried herb. Celery is a staple in many soups.</description>
		
		<graphicData>

			<texPath>Things/Items/Raw/VCE_Celery</texPath>

		</graphicData>
		<statBases>
			<MarketValue>1.1</MarketValue>
		</statBases>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>30</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
		</comps>

		<ingestible>
			<foodType>VegetableOrFruit</foodType>
		</ingestible>    
	</ThingDef>
	
