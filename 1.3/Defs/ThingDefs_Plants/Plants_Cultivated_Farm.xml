	<ThingDef ParentName="PlantBase">
		<defName>VCE_Celery</defName>
		<label>celery plant</label>
		<description>Long cultivation.</description>
		<statBases>
			<MaxHitPoints>50</MaxHitPoints>
			<Nutrition>0.15</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/Crops/VCE_Celery</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<selectable>true</selectable>
		<pathCost>14</pathCost>
		<ingestible />
		<plant>
			<fertilityMin>1</fertilityMin>
			<dieIfLeafless>true</dieIfLeafless>
			<harvestTag>Standard</harvestTag>
			<harvestedThingDef>VCE_RawCelery</harvestedThingDef>
			<harvestYield>50</harvestYield>
			<sowTags>
				<li>Ground</li>
			</sowTags>
			<topWindExposure>0.1</topWindExposure>
			<growDays>30</growDays>			
			<visualSizeRange>0.3~1.4</visualSizeRange>
			<wildOrder>2</wildOrder>
			<purpose>Food</purpose>
			<humanFoodPlant>true</humanFoodPlant>
		</plant>
	</ThingDef>
	
